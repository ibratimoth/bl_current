<script>
    function ValidateEmail()
        {
            var emailInput = document.getElementById('inputEmail4comp').value;
            var mailformat = /^\w+([\.-]?\w+)*@\w+([\.-]?\w+)*(\.\w{2,3})+$/;
            if(emailInput.match(mailformat))
            {
                $('.emailValidator').hide()
                return true;
            }
            else
            {
                $('.emailValidator').show()
                return false;
            }
        }
</script>
<script>
