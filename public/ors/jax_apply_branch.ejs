                            taxpayerBN.value = response.ownerNames[i].ownerName;
                        }
                    }
                   txtBox.value = response.businessName;
                   txtBox1.value = response.BusinessType;
                   txtBox2.value = response.registryDate;
                   $(".BNLoader").hide();
                   $(".BNFirst").show();
                }else{
                    swal({
                            type: 'error',
                            title: 'Oops...',
                            text: 'Business Name has no Owners, Please Try Again!',
                            padding: '2em',
                            timer: 3000
                        })  
                }
                }
            }
        });
    }
