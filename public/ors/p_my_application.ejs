<%- include('sidebar.ejs')%>

<!--  BEGIN CONTENT AREA  -->
<div id="content" class="main-content">
<div class="layout-px-spacing">

<div class="row layout-top-spacing" id="cancel-row">

    <div class="col-xl-12 col-lg-12 col-sm-12  layout-spacing">
        <div class="widget-content widget-content-area br-6">
            <div class="table-responsive mb-4 mt-4">
                <table class="multi-table table table-hover" id="applicationId" style="width:100%">
                    <thead>
                        <tr>
                            <th>Tracking Number</th>
                            <th>Created Date</th>
                            <th>Status</th>
                            <th class="text-center">Action</th>
                        </tr>
                    </thead>
                    <tbody>
                        
                
                    </tbody>
                    <tfoot>
                        <tr>
                            <th>Tracking Number</th>
                            <th>Created Date</th>
                            <th>Status</th>
                            <th>Action</th>
                        </tr>
                    </tfoot>
                </table>
            </div>
        </div>
    </div>

</div>

</div>
<script type="text/javascript">
setTimeout(
function myApp()
{ 
$.ajax({
    url: 'POnGoingApp',
    dataType: 'json',
    success: function(data) {
        // alert(data)
        $("#applicationId").find('tbody').empty();
            for (var i=0; i<data.length; i++) {
                var row = $('<tr><td class="text-black">' + data[i].trackingNo+ '</td>'+
                    '<td class="text-black">' + data[i].createdDate + '</td>'+
                    '<td class="text-center text-black">'+
                        '<div class="d-flex">'+
                            '<p class="align-self-center mb-0 admin-name"> Incomplete Application </p></div></td>'+
                    '<td class="text-center">'+
                        
                        ' <button class="btn btn-danger text-white"><a href="/ViewBL/'+ data[i].request_id +'" class="text-white">Proceed</a></button>'+
                    
                        ' </td></tr>');
                $('#applicationId').append(row);
            }
    },
    error: function(jqXHR, textStatus, errorThrown){
        alert('Error: ' + textStatus + ' - ' + errorThrown);
    }
});
}, 1000);
</script>
